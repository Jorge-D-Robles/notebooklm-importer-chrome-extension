{
  "manifest_version": 3,
  "name": "NotebookLM Importer",
  "version": "1.0",
  "description": "Add tabs to your NotebookLM notebook.",
  "permissions": [
    "tabs",
    "activeTab",
    "scripting",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://notebooklm.google.com/*"],
      "js": ["content.js"]
    }
  ]
}
