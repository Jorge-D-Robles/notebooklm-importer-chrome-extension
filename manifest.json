{
  "manifest_version": 3,
  "name": "NotebookLM Importer",
  "version": "1.0",
  "description": "Add tabs to your NotebookLM notebook.",
  "permissions": [
    "tabs",
    "activeTab",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "https://notebooklm.google.com/*"
  ],
  "action": {
    "default_popup": "popup.html"
  }
}
