{
  "manifest_version": 3,
  "name": "NotebookLM Importer",
  "version": "0.0.1",
  "description": "Add tabs to your NotebookLM notebook.",
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "https://notebooklm.google.com/*"
  ],
  "action": {
    "default_popup": "popup.html"
  }
}
